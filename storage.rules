rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Povolení přístupu pouze přihlášeným uživatelům
    match /doklady/{firmaNazev}/{year}/{fileName} {
      // Povolení čtení a zápisu pouze přihlášeným uživatelům
      allow read, write: if request.auth != null;
    }
  }
}
